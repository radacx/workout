{
  "name": "workout",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "windows": "concurrently -r \"npm run watch\" \"react-native run-windows\"",
    "android": "concurrently -r \"npm run watch\" \"react-native run-android\"",
    "tsc": "tsc",
    "clean": "rimraf dist",
    "watch": "npm run tsc -- -w",
    "build": "npm run clean && npm run tsc --",
    "release": "cd android && gradlew assembleRelease"
  },
  "dependencies": {
    "immutable-assign": "^2.0.4",
    "react": "^16.2.0",
    "react-native": "^0.53.0",
    "react-native-datepicker": "^1.6.0",
    "react-native-navigation": "^1.1.384",
    "react-native-radio-buttons": "^1.0.0",
    "react-native-simple-radio-button": "^2.7.1",
    "react-native-vector-icons": "^4.5.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-persist": "^5.9.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@types/react": "^16.0.38",
    "@types/react-native": "^0.52.12",
    "@types/react-native-datepicker": "^1.6.2",
    "@types/react-native-navigation": "^1.1.3",
    "@types/react-redux": "^5.0.15",
    "@types/redux": "^3.6.31",
    "@types/redux-devtools-extension": "^2.13.2",
    "babel-eslint": "^8.2.1",
    "babel-jest": "22.2.2",
    "babel-preset-react-native": "4.0.0",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "jest": "22.2.2",
    "react-test-renderer": "16.2.0",
    "redux-devtools-extension": "^2.13.2",
    "tslint": "^5.9.1",
    "typescript": "^2.8.0-dev.20180314"
  },
  "jest": {
    "preset": "react-native"
  }
}
